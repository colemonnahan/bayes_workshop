model {
  #Priors
  theta~dnorm(mu, 1/(sigma*sigma))
  beta1~dnorm(0,2)
  beta2~dnorm(0,2)
  # likelihood
  for(i in 1:R){
     p[i] <- ilogit(theta + beta1*x1[i] + beta2*x2[i])
     y[i] ~ dbin(p[i], N[i]) 
  }
  # posterior predictive distribution
  for(i in 1:R){
  # generate 1 for each replicated data set
     ypred[i] ~ dbin(p[i],N[i])
  }
}